#Starter Code

function draw() {
  background(220);
  line(100, 200, 20, 75);
describe(
  'a 100 pixels long line running from mid-top to bottom-right of canvas'
);
  rect(10,20,25,20)
  describe(
  'a 25 pixel x 20 pixel rectangle in the top left of the canvas'
);
  triangle(30, 60, 90, 60, 60, 10)
  describe(
  'a triangle in the top left of the canvas'
);
  
}
